<launch>
    <node name="joystick" pkg="joy" type="joy_node" output="screen" />
    <node name="joy_active" pkg="hamilton_ac" type="joy_active.py" output="screen" />
    <node name="ref_node" pkg="hamilton_ac" type="joy_ref.py" output="screen">
        <param name="max_lin_vel" value="1.0"/>
        <param name="max_ang_vel" value="1.0"/>
        <param name="lin_accel_scale" value="5.0"/>
        <param name="ang_accel_scale" value="5.0"/>
        <param name="vel_decay" value="5.0" />
        <param name="max_delay" value="1.0"/>
    </node>
    <param name="use_sim_time" value="false"/>
    <param name="/ac/L" value="0.0" />
    <param name="/ac/Kd" value="1.0" />
    <param name="/ac/Gamma" value="0.1" />
    <param name="/ac/q_filt" value="0.95" />
    <param name="/ac/dq_filt" value="0.95" />

    <param name="/ouijabot1/offset_angle" value="-1.57" />
    <param name="/ouijabot2/offset_angle" value="3.14" />
    <param name="/ouijabot3/offset_angle" value="1.57" />
    <param name="/ouijabot4/offset_angle" value="1.57" />
    <param name="/ouijabot5/offset_angle" value="-0.52" />
    <param name="/ouijabot6/offset_angle" value="-0.52" />

    <!-- ADD OFFSET ANGLES HERE />
</launch>
